#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/next-on-pages/bin/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/next-on-pages/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/next-on-pages/bin/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/next-on-pages/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules/@cloudflare/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/@cloudflare+next-on-pages@1.13.16_@cloudflare+workers-types@4.20251008.0_bufferutil@4.0_f7d3180479dd682456da67099ae6a285/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cloudflare/next-on-pages/bin/index.js" "$@"
else
  exec node  "$basedir/../@cloudflare/next-on-pages/bin/index.js" "$@"
fi
