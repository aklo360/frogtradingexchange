#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aklo/dev/ftx/node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aklo/dev/ftx/node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules:/Users/aklo/dev/ftx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_bufferutil@4.0.9_react@19.1.0_utf-8-validate@5.0.10/node_modules/react-native/cli.js" "$@"
fi
